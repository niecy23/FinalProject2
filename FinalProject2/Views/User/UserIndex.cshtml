@model IEnumerable<User>

<h2>Users</h2>
<table class="table">
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email Address</th>
        <th>Phone Number</th>
    </tr>

    @foreach (var user in @Model)
    {
        <tr>
            <td>@user.UserId</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.EmailAddress</td>
            <td>@user.PhoneNumber</td>
        </tr>
    }

</table>

